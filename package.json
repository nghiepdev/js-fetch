{
  "name": "js-fetch",
  "version": "1.1.0",
  "description": "Fetch JS from CDN",
  "main": "dist/js-fetch.min.js",
  "scripts": {
    "dev": "watch 'npm run build' src",
    "build": "babel src -d dist",
    "build:prod": "cross-env NODE_ENV=production babel src/js-fetch.js -o dist/js-fetch.min.js -s",
    "prepublish": "npm run build && npm run build:prod"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-babili": "^0.0.9",
    "babel-preset-env": "^1.3.3",
    "cross-env": "^4.0.0",
    "watch": "^1.0.2"
  },
  "author": "Nghiep <me@nghiepit.pro>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tronghiep92/js-fetch.git"
  },
  "bugs": {
    "url": "https://github.com/tronghiep92/js-fetch/issues"
  },
  "homepage": "http://nghiepit.pro",
  "keywords": [
    "fetch js",
    "script loader",
    "cdn"
  ],
  "license": "MIT"
}
